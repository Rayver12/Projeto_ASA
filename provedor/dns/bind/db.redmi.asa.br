;
; BIND data file for local loopback interface
;
$ORIGIN redmi.asa.br.
$TTL	604800

@	IN	SOA	@ root (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ns
@	IN	A	192.168.171.190
	IN	MX	10	mail

ns	IN	A	192.168.171.190
mail	IN	A	192.168.171.190
www	IN	A	192.168.171.190
proxy	IN	CNAME	www
pico	IN	A 	192.168.171.138